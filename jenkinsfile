pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

    stages {
        stage('Get Code') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/MauriBR/seleniumTest.git'
            }
        }

        stage('Build') {
            steps {
                // Run the maven build
                bat script : 'mvn compile'
            }
        }

        stage ('Run Test') {
            steps {
                // Run the maven tests
                bat script : 'mvn test -Dbrwoser=localchrome'
            }
        }
    }
}